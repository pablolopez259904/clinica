{
  "expo": {
    "name": "react-native-farmacia",
    "slug": "react-native-farmacia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "android":{
       "package": "umg.farmacia"
    },
    "ios": {
      "supportsTablet": true
    }, 
    
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
